{
  "name": "core-pipeline",
  "version": "1.0.0",
  "description": "Core Pipeline REST API with observability",
  "main": "dist/main.js",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "NODE_ENV=test jest --passWithNoTests --forceExit --detectOpenHandles",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage --passWithNoTests",
    "test:debug": "NODE_ENV=test node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./jest-e2e.config.js --forceExit",
    "typeorm": "typeorm-ts-node-commonjs -d ./typeorm.config.ts",
    "migration:generate": "npm run typeorm migration:generate -- -n",
    "migration:create": "npm run typeorm migration:create -- -n",
    "migration:run": "npm run typeorm migration:run",
    "migration:revert": "npm run typeorm migration:revert",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "test:showcase:dev": "ts-node src/test/showcase/dev.showcase.ts",
    "test:showcase:staging": "ts-node src/test/showcase/staging.showcase.ts",
    "test:showcase:prod": "ts-node src/test/showcase/production.showcase.ts",
    "test:all": "ts-node src/test/run-all-tests.ts",
    "test:performance": "jest --testPathPattern=performance --maxWorkers=1",
    "format:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "wait-for-postgres": "node scripts/wait-for-services.js postgres",
    "wait-for-redis": "node scripts/wait-for-services.js redis",
    "wait-for-kafka": "node scripts/wait-for-services.js kafka",
    "wait-for-all": "node scripts/wait-for-services.js all",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:down": "docker-compose -f docker-compose.dev.yml down",
    "docker:logs": "docker-compose -f docker-compose.dev.yml logs -f",
    "docker:test": "docker-compose -f docker-compose.dev.yml up -d && npm run wait-for-all && npm run test:e2e && docker-compose -f docker-compose.dev.yml down"
  },
  "keywords": [
    "nestjs",
    "observability",
    "prometheus",
    "opentelemetry"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@nestjs/bull": "^11.0.3",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.6",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.64.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.205.0",
    "@opentelemetry/instrumentation-express": "^0.54.0",
    "@opentelemetry/instrumentation-http": "^0.205.0",
    "@opentelemetry/resources": "^2.1.0",
    "@opentelemetry/sdk-metrics": "^2.1.0",
    "@opentelemetry/sdk-node": "^0.205.0",
    "@opentelemetry/sdk-trace-node": "^2.1.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "@types/bull": "^3.15.9",
    "bull": "^4.16.5",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "ioredis": "^5.8.0",
    "kafkajs": "^2.2.4",
    "pg": "^8.16.3",
    "pino": "^9.12.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.1.6",
    "@types/express": "^5.0.3",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "axios": "^1.12.2",
    "chalk": "^5.6.2",
    "dotenv": "^17.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^30.2.0",
    "prettier": "^2.8.8",
    "sqlite3": "^5.1.7",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  }
}
