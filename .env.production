# Application
NODE_ENV=production
PORT=3000
ENABLE_SWAGGER=true

# Database (Update with actual production values)
DB_HOST=postgres
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=${DB_PASSWORD}
DB_DATABASE=core_pipeline_prod
DB_LOGGING=false

# Redis (Update with actual production values)
REDIS_HOST=redis
REDIS_PORT=6379

# Kafka (Update with actual production values)
KAFKA_CLIENT_ID=core-pipeline-prod
KAFKA_BROKER=kafka:9092
KAFKA_CONSUMER_GROUP=core-pipeline-prod-group

# Observability
OTEL_EXPORTER_OTLP_ENDPOINT=http://jaeger:4318
OTEL_SERVICE_NAME=core-pipeline-prod
LOG_LEVEL=info

# Domain
APP_URL=https://core-pipeline.theedgestory.org